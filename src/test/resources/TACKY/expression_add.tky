Program("main")

// Function
// Name: "main"
// Global: true
// Parameters: None
// Statements: [ ... ]
Function("main", true, [

    compare = Var("compare")

    tmp.0 = Var("tmp.0")
    tmp.0 = Constant(ConstInt(20)) + Constant(ConstInt(30))

    // check if tmp.0 stores the value 50
    compare = tmp.0 == 50
    jumpIfNotZero(compare, end)
    printf("Should contain 50!")
    return(Constant(ConstInt(1))) // non-zero return value equals error

Label(end)
    printf("done")
    return(Constant(ConstInt(0)))

])